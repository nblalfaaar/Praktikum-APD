    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="NamaLengkap, NamaPanggilan, Prodi, Fakultas, Universitas" type="String" array="False" size=""/>
            <declare name="NIM, TigaAngkaTerakhirNIM, Modulus" type="Integer" array="False" size=""/>
            <declare name="BeratBadan" type="Real" array="False" size=""/>
            <output expression="&quot;Nama lengkap :&quot;" newline="True"/>
            <input variable="NamaLengkap"/>
            <output expression="&quot;Nama panggilan :&quot;" newline="True"/>
            <input variable="NamaPanggilan"/>
